package test:caller;


interface api {

  record new-type {
    value: string
  }

  record another-type {
    value: string
  }

  type new-type-alias = option<string>;

  type another-type-alias = result<new-type-alias, string>;

  type failing = result<_, string>;

  type not-failing = result<string>;

  type no-result = result;

  run: func(name: string) -> new-type;
}

world caller {
  
  export api;
}
